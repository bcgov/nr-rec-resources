{
  "name": "quickstart-openshift-react-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "clean": "rimraf ./node_modules/.vite",
    "deploy": "npm ci --ignore-scripts --no-update-notifier --omit=dev && npm run build",
    "dev": "vite --host",
    "e2e": "npx happo-e2e -- npx playwright test",
    "e2e:ui": "npx happo-e2e -- npx playwright test --ui",
    "preview": "vite preview",
    "test": "vitest --mode test",
    "test:cov": "vitest run --mode test --coverage"
  },
  "dependencies": {
    "@bcgov/bc-sans": "^2.1.0",
    "@bcgov/design-system-react-components": "^0.4.0",
    "@digitalspace/bcparks-bootstrap-theme": "^1.4.6",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@vitejs/plugin-react": "^4.3.4",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.7",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.7",
    "react-dom": "^18.3.1",
    "react-lazy-load-image-component": "^1.6.3",
    "react-router": "^7.0.0",
    "react-router-dom": "^7.0.0",
    "react-use-scrollspy": "^3.1.1",
    "yet-another-react-lightbox": "^3.21.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@faker-js/faker": "^9.0.0",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.1",
    "happo-e2e": "^3.2.0",
    "happo-playwright": "^2.1.0",
    "happo.io": "^11.0.0",
    "history": "^5.3.0",
    "jsdom": "^25.0.0",
    "msw": "^2.4.4",
    "sass-embedded": "^1.83.0",
    "typescript": "^5.1.3",
    "vite": "^6.0.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.5"
  },
  "overrides": {
    "rollup@<4.22.4": "^4.22.4"
  },
  "optionalDependencies": {
    "sass-embedded-linux-x64": "^1.83.0"
  }
}
