{
  "name": "public-frontend",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "clean": "rimraf ./node_modules/.vite",
    "deploy": "npm ci --ignore-scripts --no-update-notifier && npm run build && npm prune --production",
    "dev": "vite --host",
    "e2e": "npx happo-e2e -- npx playwright test",
    "e2e:chrome": "npx happo-e2e -- npx playwright test --project=chromium",
    "e2e:ui": "npx happo-e2e -- npx playwright test --ui",
    "preview": "vite preview",
    "test": "vitest --mode test",
    "test:cov": "npx vitest run --mode test --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
